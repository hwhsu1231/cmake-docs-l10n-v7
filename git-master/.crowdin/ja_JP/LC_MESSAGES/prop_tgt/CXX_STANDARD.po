#: ../../../prop_tgt/CXX_STANDARD.rst:2
msgid "CXX_STANDARD"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:6
msgid "The C++ standard whose features are requested to build this target."
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:8
msgid "This property specifies the C++ standard whose features are requested to build this target.  For some compilers, this results in adding a flag such as ``-std=gnu++11`` to the compile line.  For compilers that have no notion of a standard level, such as Microsoft Visual C++ before 2015 Update 3, this has no effect."
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:14
msgid "Supported values are:"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:17
msgid "``98``"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:17
msgid "C++98"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:20
msgid "``11``"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:20
msgid "C++11"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:23
msgid "``14``"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:23
msgid "C++14"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:28
msgid "``17``"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:28
msgid "C++17"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:33
msgid "``20``"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:33
msgid "C++20"
msgstr ""

#: ../../../prop_tgt/CXX_STANDARD.rst:38
msgid "``23``"
msgstr ""

