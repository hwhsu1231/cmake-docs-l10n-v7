msgid "ANDROID_STL_TYPE"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:6
msgid "When :ref:`Cross Compiling for Android with NVIDIA Nsight Tegra Visual Studio Edition`, this property specifies the type of STL support for the project. This is a string property that could set to the one of the following values:"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:10
msgid "``none``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:11
msgid "No C++ Support"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:12
msgid "``system``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:13
msgid "Minimal C++ without STL"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:14
msgid "``gabi++_static``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:15
msgid "GAbi++ Static"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:16
msgid "``gabi++_shared``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:17
msgid "GAbi++ Shared"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:18
msgid "``gnustl_static``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:19
msgid "GNU libstdc++ Static"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:20
msgid "``gnustl_shared``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:21
msgid "GNU libstdc++ Shared"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:22
msgid "``stlport_static``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:23
msgid "STLport Static"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:25
msgid "``stlport_shared``"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:25
msgid "STLport Shared"
msgstr ""

#: ../../../prop_tgt/ANDROID_STL_TYPE.rst:27
msgid "This property is initialized by the value of the :variable:`CMAKE_ANDROID_STL_TYPE` variable if it is set when a target is created."
msgstr ""

