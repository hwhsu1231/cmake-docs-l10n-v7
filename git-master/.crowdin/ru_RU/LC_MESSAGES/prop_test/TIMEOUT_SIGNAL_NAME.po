#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:2
msgid "TIMEOUT_SIGNAL_NAME"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:6
msgid "Specify a custom signal to send to a test process when its timeout is reached. This is available only on platforms supporting POSIX signals. It is not available on Windows."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:10
msgid "The name must be one of the following:"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:13
msgid "``SIGINT``"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:13
msgid "Interrupt."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:16
msgid "``SIGQUIT``"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:16
msgid "Quit."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:19
msgid "``SIGTERM``"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:19
msgid "Terminate."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:22
msgid "``SIGUSR1``"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:22
msgid "User defined signal 1."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:25
msgid "``SIGUSR2``"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:25
msgid "User defined signal 2."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:27
msgid "The custom signal is sent to the test process to give it a chance to exit gracefully during a grace period:"
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:30
msgid "If the test process created any children, it is responsible for terminating them too."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:33
msgid "The grace period length is determined by the :prop_test:`TIMEOUT_SIGNAL_GRACE_PERIOD` test property."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:36
msgid "If the test process does not terminate before the grace period ends, :manual:`ctest(1)` will force termination of its entire process tree via ``SIGSTOP`` and ``SIGKILL``."
msgstr ""

#: ../../../prop_test/TIMEOUT_SIGNAL_NAME.rst:40
msgid "See also :variable:`CTEST_TEST_TIMEOUT`, :prop_test:`TIMEOUT`, and :prop_test:`TIMEOUT_AFTER_MATCH`."
msgstr ""

