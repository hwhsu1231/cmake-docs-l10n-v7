msgid "CPACK_WIX_ACL"
msgstr ""

#: ../../../prop_inst/CPACK_WIX_ACL.rst:4
msgid "Specifies access permissions for files or directories installed by a WiX installer."
msgstr ""

#: ../../../prop_inst/CPACK_WIX_ACL.rst:7
msgid "The property can contain multiple list entries, each of which has to match the following format."
msgstr ""

#: ../../../prop_inst/CPACK_WIX_ACL.rst:12
msgid "<user>[@<domain>]=<permission>[,<permission>]"
msgstr ""

#: ../../../prop_inst/CPACK_WIX_ACL.rst:14
msgid "``<user>`` and ``<domain>`` specify the windows user and domain for which the ``<Permission>`` element should be generated."
msgstr ""

#: ../../../prop_inst/CPACK_WIX_ACL.rst:17
msgid "``<permission>`` is any of the YesNoType attributes listed here::"
msgstr ""

#: ../../../prop_inst/CPACK_WIX_ACL.rst:19
