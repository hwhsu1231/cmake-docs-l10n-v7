#: ../../../prop_tgt/CUDA_RESOLVE_DEVICE_SYMBOLS.rst:2
msgid "CUDA_RESOLVE_DEVICE_SYMBOLS"
msgstr ""

#: ../../../prop_tgt/CUDA_RESOLVE_DEVICE_SYMBOLS.rst:4
msgid "CUDA only: Enables device linking for the specific static library target"
msgstr ""

#: ../../../prop_tgt/CUDA_RESOLVE_DEVICE_SYMBOLS.rst:6
msgid "If set this will enable device linking on this static library target. Normally device linking is deferred until a shared library or executable is generated, allowing for multiple static libraries to resolve device symbols at the same time."
msgstr ""

#: ../../../prop_tgt/CUDA_RESOLVE_DEVICE_SYMBOLS.rst:11
msgid "For instance:"
msgstr ""

#: ../../../prop_tgt/CUDA_RESOLVE_DEVICE_SYMBOLS.rst:13
msgid "set_property(TARGET mystaticlib PROPERTY CUDA_RESOLVE_DEVICE_SYMBOLS ON)"
msgstr ""

