#: ../../../../Modules/FindGTK2.cmake:6
msgid "FindGTK2"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:8
msgid "FindGTK2.cmake"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:10
msgid "This module can find the GTK2 widget libraries and several of its other optional components like gtkmm, glade, and glademm."
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:13
msgid "NOTE: If you intend to use version checking, CMake 2.6.2 or later is"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:17
msgid "required."
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:21
msgid "Specify one or more of the following components as you call this find module.  See example below."
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:26
msgid "gtk\n"
"gtkmm\n"
"glade\n"
"glademm"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:33
msgid "The following variables will be defined for your use"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:37
msgid "GTK2_FOUND - Were all of your specified components found?\n"
"GTK2_INCLUDE_DIRS - All include directories\n"
"GTK2_LIBRARIES - All libraries\n"
"GTK2_TARGETS - All imported targets\n"
"GTK2_DEFINITIONS - Additional compiler flags"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:47
msgid "GTK2_VERSION - The version of GTK2 found (x.y.z)\n"
"GTK2_MAJOR_VERSION - The major version of GTK2\n"
"GTK2_MINOR_VERSION - The minor version of GTK2\n"
"GTK2_PATCH_VERSION - The patch version of GTK2"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:54
msgid "Optional variables you can define prior to calling this module:"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:58
msgid "GTK2_DEBUG - Enables verbose debugging of the module\n"
"GTK2_ADDITIONAL_SUFFIXES - Allows defining additional directories to\n"
"                           search for include files"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:64
msgid "================= Example Usage:"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:68
msgid "Call find_package() once, here are some examples to pick from:"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:74
msgid "Require GTK 2.6 or later\n"
"    find_package(GTK2 2.6 REQUIRED gtk)"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:81
msgid "Require GTK 2.10 or later and Glade\n"
"    find_package(GTK2 2.10 REQUIRED gtk glade)"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:88
msgid "Search for GTK/GTKMM 2.8 or later\n"
"    find_package(GTK2 2.8 COMPONENTS gtk gtkmm)"
msgstr ""

#: ../../../../Modules/FindGTK2.cmake:95
msgid "if(GTK2_FOUND)\n"
"   include_directories(${GTK2_INCLUDE_DIRS})\n"
"   add_executable(mygui mygui.cc)\n"
"   target_link_libraries(mygui ${GTK2_LIBRARIES})\n"
"endif()"
msgstr ""

