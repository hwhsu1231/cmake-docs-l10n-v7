#: ../../../variable/CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS.rst:2
msgid "CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS"
msgstr ""

#: ../../../variable/CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS.rst:4
msgid "This variable contains a list of env vars as a list of tokens with the syntax ``var=value``."
msgstr ""

#: ../../../variable/CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS.rst:7
msgid "Example:"
msgstr ""

#: ../../../variable/CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS.rst:9
msgid "set(CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS\n"
"   \"FOO=FOO1\\;FOO2\\;FOON\"\n"
"   \"BAR=BAR1\\;BAR2\\;BARN\"\n"
"   \"BAZ=BAZ1\\;BAZ2\\;BAZN\"\n"
"   \"FOOBAR=FOOBAR1\\;FOOBAR2\\;FOOBARN\"\n"
"   \"VALID=\"\n"
"   )"
msgstr ""

#: ../../../variable/CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS.rst:19
msgid "In case of malformed variables CMake will fail:"
msgstr ""

#: ../../../variable/CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS.rst:21
msgid "set(CMAKE_SUBLIME_TEXT_2_ENV_SETTINGS\n"
"    \"THIS_IS_NOT_VALID\"\n"
"    )"
msgstr ""

