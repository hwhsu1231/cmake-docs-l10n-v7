#: ../../../../Modules/FindLua.cmake:6
msgid "FindLua"
msgstr ""

#: ../../../../Modules/FindLua.cmake:10
msgid "Locate Lua library This module defines"
msgstr ""

#: ../../../../Modules/FindLua.cmake:14
msgid "LUA_FOUND          - if false, do not try to link to Lua\n"
"LUA_LIBRARIES      - both lua and lualib\n"
"LUA_INCLUDE_DIR    - where to find lua.h\n"
"LUA_VERSION_STRING - the version of Lua found\n"
"LUA_VERSION_MAJOR  - the major version of Lua\n"
"LUA_VERSION_MINOR  - the minor version of Lua\n"
"LUA_VERSION_PATCH  - the patch version of Lua"
msgstr ""

#: ../../../../Modules/FindLua.cmake:24
msgid "Note that the expected include convention is"
msgstr ""

#: ../../../../Modules/FindLua.cmake:28
msgid "#include \"lua.h\""
msgstr ""

#: ../../../../Modules/FindLua.cmake:30
msgid "and not"
msgstr ""

#: ../../../../Modules/FindLua.cmake:34
msgid "#include <lua/lua.h>"
msgstr ""

#: ../../../../Modules/FindLua.cmake:36
msgid "This is because, the lua location is not standardized and may exist in locations other than lua/"
msgstr ""

