#: ../../../../Modules/CMakePrintHelpers.cmake:6
msgid "CMakePrintHelpers"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:8
msgid "Convenience macros for printing properties and variables, useful e.g. for debugging."
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:12
msgid "CMAKE_PRINT_PROPERTIES([TARGETS target1 ..  targetN]\n"
"                       [SOURCES source1 .. sourceN]\n"
"                       [DIRECTORIES dir1 .. dirN]\n"
"                       [TESTS test1 .. testN]\n"
"                       [CACHE_ENTRIES entry1 .. entryN]\n"
"                       PROPERTIES prop1 .. propN )"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:19
msgid "This macro prints the values of the properties of the given targets, source files, directories, tests or cache entries.  Exactly one of the scope keywords must be used.  Example::"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:23
msgid "cmake_print_properties(TARGETS foo bar PROPERTIES\n"
"                       LOCATION INTERFACE_INCLUDE_DIRS)"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:26
msgid "This will print the LOCATION and INTERFACE_INCLUDE_DIRS properties for both targets foo and bar."
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:31
msgid "CMAKE_PRINT_VARIABLES(var1 var2 ..  varN)"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:33
msgid "This macro will print the name of each variable followed by its value. Example::"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:36
msgid "cmake_print_variables(CMAKE_C_COMPILER CMAKE_MAJOR_VERSION DOES_NOT_EXIST)"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:38
msgid "Gives::"
msgstr ""

#: ../../../../Modules/CMakePrintHelpers.cmake:40
msgid "-- CMAKE_C_COMPILER=\"/usr/bin/gcc\" ; CMAKE_MAJOR_VERSION=\"2\" ; DOES_NOT_EXIST=\"\""
msgstr ""

