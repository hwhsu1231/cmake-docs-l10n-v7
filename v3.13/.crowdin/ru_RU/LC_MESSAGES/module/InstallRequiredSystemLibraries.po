#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:6
msgid "InstallRequiredSystemLibraries"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:8
msgid "Include this module to search for compiler-provided system runtime libraries and add install rules for them.  Some optional variables may be set prior to including the module to adjust behavior:"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:14
msgid "``CMAKE_INSTALL_SYSTEM_RUNTIME_LIBS``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:13
msgid "Specify additional runtime libraries that may not be detected. After inclusion any detected libraries will be appended to this."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:19
msgid "``CMAKE_INSTALL_SYSTEM_RUNTIME_LIBS_SKIP``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:17
msgid "Set to TRUE to skip calling the :command:`install(PROGRAMS)` command to allow the includer to specify its own install rule, using the value of ``CMAKE_INSTALL_SYSTEM_RUNTIME_LIBS`` to get the list of libraries."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:23
msgid "``CMAKE_INSTALL_DEBUG_LIBRARIES``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:22
msgid "Set to TRUE to install the debug runtime libraries when available with MSVC tools."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:27
msgid "``CMAKE_INSTALL_DEBUG_LIBRARIES_ONLY``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:26
msgid "Set to TRUE to install only the debug runtime libraries with MSVC tools even if the release runtime libraries are also available."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:37
msgid "``CMAKE_INSTALL_UCRT_LIBRARIES``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:30
msgid "Set to TRUE to install the Windows Universal CRT libraries for app-local deployment (e.g. to Windows XP).  This is meaningful only with MSVC from Visual Studio 2015 or higher."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:34
msgid "One may set a ``CMAKE_WINDOWS_KITS_10_DIR`` *environment variable* to an absolute path to tell CMake to look for Windows 10 SDKs in a custom location.  The specified directory is expected to contain ``Redist/ucrt/DLLs/*`` directories."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:40
msgid "``CMAKE_INSTALL_MFC_LIBRARIES``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:40
msgid "Set to TRUE to install the MSVC MFC runtime libraries."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:43
msgid "``CMAKE_INSTALL_OPENMP_LIBRARIES``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:43
msgid "Set to TRUE to install the MSVC OpenMP runtime libraries"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:48
msgid "``CMAKE_INSTALL_SYSTEM_RUNTIME_DESTINATION``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:46
msgid "Specify the :command:`install(PROGRAMS)` command ``DESTINATION`` option.  If not specified, the default is ``bin`` on Windows and ``lib`` elsewhere."
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:53
msgid "``CMAKE_INSTALL_SYSTEM_RUNTIME_LIBS_NO_WARNINGS``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:51
msgid "Set to TRUE to disable warnings about required library files that do not exist.  (For example, Visual Studio Express editions may not provide the redistributable files.)"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:734
msgid "``CMAKE_INSTALL_SYSTEM_RUNTIME_COMPONENT``"
msgstr ""

#: ../../../../Modules/InstallRequiredSystemLibraries.cmake:56
msgid "Specify the :command:`install(PROGRAMS)` command ``COMPONENT`` option.  If not specified, no such option will be used."
msgstr ""

