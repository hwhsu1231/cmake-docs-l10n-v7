#: ../../../variable/CMAKE_FIND_PACKAGE_SORT_DIRECTION.rst:2
msgid "CMAKE_FIND_PACKAGE_SORT_DIRECTION"
msgstr ""

#: ../../../variable/CMAKE_FIND_PACKAGE_SORT_DIRECTION.rst:4
msgid "The sorting direction used by :variable:`CMAKE_FIND_PACKAGE_SORT_ORDER`. It can assume one of the following values:"
msgstr ""

#: ../../../variable/CMAKE_FIND_PACKAGE_SORT_DIRECTION.rst:9
msgid "``DEC``"
msgstr ""

#: ../../../variable/CMAKE_FIND_PACKAGE_SORT_DIRECTION.rst:8
msgid "Default.  Ordering is done in descending mode. The highest folder found will be tested first."
msgstr ""

#: ../../../variable/CMAKE_FIND_PACKAGE_SORT_DIRECTION.rst:13
msgid "``ASC``"
msgstr ""

#: ../../../variable/CMAKE_FIND_PACKAGE_SORT_DIRECTION.rst:12
msgid "Ordering is done in ascending mode. The lowest folder found will be tested first."
msgstr ""

#: ../../../variable/CMAKE_FIND_PACKAGE_SORT_DIRECTION.rst:15
msgid "If :variable:`CMAKE_FIND_PACKAGE_SORT_ORDER` is not set or is set to ``NONE`` this variable has no effect."
msgstr ""

