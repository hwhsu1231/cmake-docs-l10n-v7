#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:2
msgid "CMAKE_INSTALL_MESSAGE"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:4
msgid "Specify verbosity of installation script code generated by the :command:`install` command (using the :command:`file(INSTALL)` command). For paths that are newly installed or updated, installation may print lines like::"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:9
msgid "-- Installing: /some/destination/path"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:11
msgid "For paths that are already up to date, installation may print lines like::"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:14
msgid "-- Up-to-date: /some/destination/path"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:16
msgid "The ``CMAKE_INSTALL_MESSAGE`` variable may be set to control which messages are printed:"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:20
msgid "``ALWAYS``"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:20
msgid "Print both ``Installing`` and ``Up-to-date`` messages."
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:23
msgid "``LAZY``"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:23
msgid "Print ``Installing`` but not ``Up-to-date`` messages."
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:26
msgid "``NEVER``"
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:26
msgid "Print neither ``Installing`` nor ``Up-to-date`` messages."
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:28
msgid "Other values have undefined behavior and may not be diagnosed."
msgstr ""

#: ../../../variable/CMAKE_INSTALL_MESSAGE.rst:30
msgid "If this variable is not set, the default behavior is ``ALWAYS``."
msgstr ""

